{
  "name": "@eatonfyi/arangorm",
  "version": "1.0.0",
  "description": "A lightweight ArangoDB wrapper.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll",
    "clean": "shx rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "test/**",
      "!test/**/{fixtures,helpers}/**"
    ],
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "keywords": [
    "asin",
    "amazon",
    "scraper"
  ],
  "author": "eaton",
  "license": "MIT",
  "devDependencies": {
    "@types/dockerode": "^3.3.28",
    "@types/node": "^20.11.30",
    "ava": "^6.1.2",
    "dockerode": "^4.0.2",
    "execa": "^8.0.1",
    "get-port": "^7.1.0",
    "pkgroll": "^2.0.2",
    "shx": "^0.3.4",
    "tsimp": "^2.0.11",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@eatonfyi/ids": "github:eaton/ids",
    "arangojs": "^9.0.0-preview.1",
    "ts-deepmerge": "^7.0.0",
    "zod": "^3.23.4",
    "zod-to-json-schema": "^3.23.0"
  }
}
